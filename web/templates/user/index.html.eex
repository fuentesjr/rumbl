<h1>Users</h1>
<h4 class="pull-right"><%= link "New User", to: user_path(@conn, :new)  %></h4>

<table class="table">
  <%= for user <- @users do %>
    <tr>
      <td><%= render "user.html", user: user %></td>
      <td><%= link "View", to: user_path(@conn, :show, user.id) %></td>
      <td><%= link "Delete", to: user_path(@conn, :delete, user.id), method: "delete"%></td>
    </tr>
  <% end %>
</table
